type Vehicle {
    id: ID! @id
    createdAt: DateTime! @createdAt
    vehicleID: String
    route: Route
    predictable: Boolean
    runID: String
    latitude: Float
    longitude: Float
    heading: Int
    timeSinceReport: Int
    dayOfWeek: String
    dataLoad: Json
}

type Route {
    id: ID! @id
    createdAt: DateTime! @createdAt
    routeID: String
    vehicles: [Vehicle] @relation(onDelete: CASCADE)
}