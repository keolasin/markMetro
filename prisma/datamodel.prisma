type Vehicle {
    id: ID! @id
    createdAt: DateTime! @createdAt
    vehicleID: String
    route: Route
    predictable: Boolean
    runID: String
    latitude: String
    longitude: String
    heading: Int
    timeSinceReport: Int
    serverTime: String
    data: Json
}

type Route {
    id: ID! @id
    createdAt: DateTime! @createdAt
    name: String
    vehicles: [Vehicle] @relation(onDelete: CASCADE)
}